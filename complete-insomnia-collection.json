        "mimeType": "application/json",
        "text": "{\n  \"username\": \"testuser\",\n  \"first_name\": \"Test\",\n  \"last_name\": \"User\",\n  \"email\": \"test@example.com\",\n  \"password\": \"SecurePass123!\",\n  \"tier\": \"dev\"\n}"
      },
      "headers": [
